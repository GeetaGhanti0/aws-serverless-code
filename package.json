{
  "name": "Sample Serverless Architecture",
  "version": "0.1.0",
  "scripts": {
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "compliance:code": "eslint . --ext js,ts --max-warnings=0 --ignore-path .gitignore",
    "compliance:code:fix": "prettier --write \"**/*.{js,json,ts,md,yml,yaml}\"",
    "compliance:documentation": "markdownlint **/*.md --ignore '**/node_modules/**'",
    "security:audit": "npm audit --production",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "cdk": "cdk"
  },
  "dependencies": {
    "@aws-cdk/aws-iam": "1.124.0",
    "@aws-cdk/aws-kms": "1.124.0",
    "@aws-cdk/aws-lambda": "1.124.0",
    "@aws-cdk/aws-lambda-event-sources": "1.124.0",
    "@aws-cdk/aws-s3": "1.124.0",
    "@aws-cdk/aws-sqs": "1.124.0",
    "@aws-cdk/aws-ssm": "1.124.0",
    "@aws-cdk/core": "1.124.0",
    "iag-dev-cdk-core": "1.13.0",
    "source-map-support": "0.5.20"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.124.0",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.9.6",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "aws-cdk": "1.124.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "jest": "27.2.1",
    "jest-junit": "12.3.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "11.1.2",
    "markdownlint-cli": "0.28.1",
    "prettier": "2.4.1",
    "source-map-support": "0.5.20",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  }
}
